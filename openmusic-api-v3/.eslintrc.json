{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "quotes": ["error", "double"],
    "linebreak-style": ["error", "unix"]
  }
}
